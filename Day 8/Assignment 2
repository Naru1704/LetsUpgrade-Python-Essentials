with open("translate.txt", "r") as f:
    print(f.read())
    try:
        f.write(input("Enter input to write\n"))
    except:
        print("Sorry, the file is open in \"READ ONLY MODE\"\n"
              "If you want to write something open file in \"WRITE OR READ AND WRTIE MODE\"")
    finally:
        print("Thank You!!!")
